{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "outDir": "./dist",
    "rootDir": "./",
    "tsBuildInfoFile": "./dist/.tsbuildinfo",
    "noEmit": false,
    "module": "commonjs",
    "emitDeclarationOnly": false
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}